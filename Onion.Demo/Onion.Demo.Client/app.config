<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IFiscalService" />
                <binding name="BasicHttpBinding_IEmployeeService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8000/FiscalService" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IFiscalService" contract="FiscalService.IFiscalService"
                name="BasicHttpBinding_IFiscalService" />
            <endpoint address="http://localhost:8000/EmployeeService" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IEmployeeService" contract="EmployeeService.IEmployeeService"
                name="BasicHttpBinding_IEmployeeService" />
        </client>
    </system.serviceModel>
</configuration>